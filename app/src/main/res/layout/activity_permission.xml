<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_fff"
    android:fitsSystemWindows="true"
    tools:context="com.luwei.lwbaselib.activity.PermissionActivity">

    <com.luwei.ui.view.TitleBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titleText="权限获取Permission"/>

    <Button
        android:id="@+id/btn_get_permission"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="获取图片处理权限"
        android:layout_gravity="center"/>


</FrameLayout>
